{
  "root": true,
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
  ],
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
